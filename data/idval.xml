<group>
  <command_dir>%DATA%/commands</command_dir>
  <command_extension>pm</command_extension>
  <data_store>%DATA%/data_store.bin</data_store>
  <demo_validate_cfg>%DATA%/val_demo.cfg</demo_validate_cfg>
  <plugin_dir>%LIB%/Plugins</plugin_dir>
  <select>
    <name>config_group</name>
    <op>==</op>
    <value>idval_settings</value>
  </select>
  <visible_separator>%%</visible_separator>
</group>
<group>
  <command_path>~/local/bin/lame.exe</command_path>
  <select>
    <name>command_name</name>
    <op>==</op>
    <value>lame</value>
  </select>
</group>
<group>
  <command_path>~/local/bin/Tag.exe</command_path>
  <select>
    <name>command_name</name>
    <op>==</op>
    <value>tag</value>
  </select>
</group>
<group>
  <command_path>/cygdrive/c/Program Files/Timidity++ Project/Timidity++/timidity.exe</command_path>
  <config_file>/cygdrive/h/local/share/freepats/crude.cfg</config_file>
  <select>
    <name>command_name</name>
    <op>==</op>
    <value>timidity</value>
  </select>
</group>
<group>
  <convert>MP3</convert>
  <group>
    <convert>MIDI</convert>
    <select>
      <name>type</name>
      <op>==</op>
      <value>ABC</value>
    </select>
  </group>
  <select>
    <name>class</name>
    <op>==</op>
    <value>MUSIC</value>
  </select>
</group>
<group>
  <group>
    <A>TEXT</A>
    <C>TCOM</C>
    <D>TALB</D>
    <K>TKEY</K>
    <T>TITLE</T>
    <X>TRACK</X>
    <Z>TENC</Z>
    <abc-copyright>TCOP</abc-copyright>
    <select>
      <name>type</name>
      <op>==</op>
      <value>ABC</value>
    </select>
  </group>
  <group>
    <TRACKNUMBER>TRACK</TRACKNUMBER>
    <select>
      <name>DATE</name>
      <op>==</op>
      <value>YEAR</value>
    </select>
    <select>
      <name>type</name>
      <op>==</op>
      <value>OGG</value>
    </select>
  </group>
  <group>
    <TRACKNUMBER>TRACK</TRACKNUMBER>
    <select>
      <name>DATE</name>
      <op>==</op>
      <value>YEAR</value>
    </select>
    <select>
      <name>type</name>
      <op>==</op>
      <value>FLAC</value>
    </select>
  </group>
  <select>
    <name>config_group</name>
    <op>==</op>
    <value>tag_mappings</value>
  </select>
</group>
<group>
  <data_store>~/.idval/data_store.bin</data_store>
  <select>
    <name>config_group</name>
    <op>==</op>
    <value>idval_settings</value>
  </select>
</group>
